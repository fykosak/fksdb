	#         - msg1
	#         - msg2
	#     - sk:
	#         - dfads
	#     - en:
	#         - dfsadfsd
    vyfuk:
        forwardRegistration: %fykos.forwardRegistration%

nette:
    database:
        dsn: 'mysql:host=127.0.0.1;dbname=newfks'
        user:
        password:



{block outerContent}
    {varType array $contestants}
    {varType array $organizers}
    {varType array $payments}
    <div class="panel color-auto">
        <div class="container">
            <h2>{_'Shortcuts'}</h2>
            {control navigationChooser:boardInline $payments, false}
        </div>
    </div>
    <div class="panel color-auto">
        <div class="container">
            <h2>{_'Contestant'}</h2>
            {control navigationChooser:boardInline $contestants, false}
        </div>
    </div>
    <div class="panel color-auto">
        <div class="container">
            {if count($organizers)}
                <h2>{_'Organizer'}</h2>
                {control navigationChooser:boardInline $organizers, false}
            {/if}
        </div>
    </div>
{/block}

{foreach $data as $section}
    {if isset($section['headline'])}
        <h2>{_($section['headline'])}</h2>
    {/if}
    {foreach $section['fields'] as $field}
        {var $def = FKSDB\Components\Forms\Factories\TableReflectionFactory::parseRow($field)}
        {control valuePrinter__$def[0]__$def[1]:row $model}
    {/foreach}
    <hr/>
{/foreach}

<ol n:if="$path" class="breadcrumb">
    <li n:foreach="$path as $request" n:class="'breadcrumb-item '.($iterator->isLast() ? 'active')">
        <a href="{$request->url}" n:tag-if="!$iterator->isLast()">
            <i class="{$request->title->icon} me-1"></i>
            {$request->title->title}</a>
    </li>
</ol>

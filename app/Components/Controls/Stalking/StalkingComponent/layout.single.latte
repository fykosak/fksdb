{layout '../@layout.latte'}
{block content}
    {if \is_null($model)}
        <div class="card-body">
            {control noRecords}
        </div>
    {else}
        {foreach $rows as $field}
            {control 'valuePrinter__'.$field[0].'__'.$field[1]:listItem $model}
        {/foreach}
    {/if}
{/block}

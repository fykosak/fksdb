{layout '../@layout.latte'}
{block content}
    {if is_null($model)}
        <div class="card-body">
            {control noRecords}
        </div>
    {else}
        {foreach $rows as $field}
            {control valuePrinter:listItem $field[0].'.'.$field[1], $model}
        {/foreach}
    {/if}
{/block}

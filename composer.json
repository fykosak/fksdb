{
  "require": {
    "php": ">=7.0.0",
    "tracy/tracy": "~2.5.5",
    "nette/utils": "~2.5.3",
    "nette/security": "~2.4.4",
    "nette/tokenizer": "~2.3.0",
    "nette/finder": "~2.4.2",
    "nette/http": "~2.4.10",
    "nette/robot-loader": "~2.4.4",
    "nette/safe-stream": "~2.3.3",
    "nette/component-model": "~2.4.0",
    "nette/mail": "~2.4.6",
    "nette/neon": "~2.4.3",
    "nette/reflection": "~2.4.2",
    "nette/php-generator": "~2.6.4",
    "nette/database": "~2.4.8",
    "latte/latte": "~2.4.8",
    "nette/tester": "~2.0.0"
  },
  "autoload": {
    "classmap": []
  },
  "scripts": {
    "initTestDatabase": [
      "./tests/init-database.sh"
    ],
    "clearTestDatabase": [
      "./tests/clear-database.sh"
    ],
    "clearTestCache": [
      "rm -rf temp/tester/"
    ],
    "clearCache": [
      "rm -rf temp/cache/"
    ],
    "test":[
      "@clearTestCache",
      "@clearTestDatabase",
      "vendor/bin/tester tests -p php --coverage coverage.html --coverage-src app -s -c tests/php.ini -j 1"
    ]
  }
}

{
  "description": "",
  "name": "fykosak/fksdb",
  "require": {
    "php": "7.4.* | 8.0.* | 8.1.*",
    "ext-gettext": "*",
    "ext-mbstring": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-soap": "*",
    "ext-dom": "*",
    "ext-xsl": "*",
    "nette/utils": "^v3.2.7",
    "nette/security": "^v3.1.5",
    "nette/finder": "^v2.5.3",
    "nette/http": "^v3.1.5",
    "nette/robot-loader": "^3.4.1",
    "nette/safe-stream": "^2.4.2",
    "nette/component-model": "^3.0.1",
    "nette/mail": "^v3.1.8",
    "nette/neon": "^v3.3.3",
    "nette/forms": "^v3.1.7",
    "nette/php-generator": "^v3.6.7",
    "nette/database": "^3.1.5",
    "nette/caching": "^3.1.3",
    "latte/latte": "^2.11.4",
    "tracy/tracy": "^v2.9.3",
    "nette/tester": "^2.4.2",
    "nette/application": "^3.1.7",
    "nette/di": "^3.0.13",
    "nette/bootstrap": "^3.1.2",
    "nette/nette": "^3.1.0",
    "setasign/fpdf": "1.8.2",
    "setasign/fpdi": "1.5.4",
    "league/oauth2-google": "3.0.3",
    "fykosak/nette-orm": "dev-dev-mapper#d6b6381",
    "fykosak/nette-fks-utils": "dev-dev-context-translator#a4a8bbb",
    "fykosak/nette-frontend-component": "dev-dev-74#d606152"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.6.0"
  },
  "authors": [
    {
      "name": "Michal Koutny",
      "email": "michal@fykos.cz",
      "role": "FKSDB Creator"
    },
    {
      "name": "Organisers of FYKOS.cz",
      "email": "webmaster@fykos.cz"
    }
  ],
  "autoload": {
    "psr-4": {
      "FKSDB\\": "app/",
      "FKSDB\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "initTestDatabase": [
      "./tests/init-database.sh"
    ],
    "clearTestDatabase": [
      "./tests/clear-database.sh"
    ],
    "clearTestCache": [
      "rm -rf temp/tester/"
    ],
    "clearCache": [
      "rm -rf temp/cache/"
    ],
    "test": [
      "@clearTestCache",
      "@clearTestDatabase",
      "vendor/bin/tester tests -p php -s -c tests/php.ini -j 8"
    ],
    "testCoverage": [
      "@clearTestCache",
      "@clearTestDatabase",
      "vendor/bin/tester tests -p php --coverage coverage.html --coverage-src app -s -c tests/php.ini -j 8"
    ],
    "testPSR12": [
      "php -e vendor/squizlabs/php_codesniffer/bin/phpcs --extensions=php,phpt --standard=PSR12 --parallel=8 app/ tests/"
    ]
  }
}

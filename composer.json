{
  "description": "",
  "name": "fykosak/fksdb",
  "require": {
    "php": "7.4.* | 8.0.* | 8.1.*",
    "ext-gettext": "*",
    "ext-mbstring": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-soap": "*",
    "ext-dom": "*",
    "ext-xsl": "*",
    "nette/utils": "v3.2.9",
    "nette/security": "v3.1.7",
    "nette/finder": "v2.6.0",
    "nette/http": "v3.2.2",
    "nette/robot-loader": "v3.4.2",
    "nette/safe-stream": "v2.5.1",
    "nette/component-model": "v3.0.3",
    "nette/mail": "v3.1.10",
    "nette/neon": "v3.3.3",
    "nette/forms": "v3.1.11",
    "nette/php-generator": "v3.6.9",
    "nette/database": "v3.1.7",
    "nette/caching": "v3.1.4",
    "latte/latte": "v2.11.6",
    "tracy/tracy": "v2.9.7",
    "nette/tester": "v2.4.3",
    "nette/application": "v3.1.11",
    "nette/di": "v3.1.2",
    "nette/bootstrap": "v3.1.4",
    "nette/nette": "^3.2.0",
    "setasign/fpdf": "v1.8.2",
    "setasign/fpdi": "v1.5.4",
    "league/oauth2-google": "v3.0.3",
    "fykosak/nette-orm": "dev-dev-mapper#d6b6381",
    "fykosak/nette-fks-utils": "dev-dev-context-translator#a4a8bbb",
    "fykosak/nette-frontend-component": "dev-dev-74#d606152"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.6.0",
    "phpstan/phpstan": "1.10.21"
  },
  "authors": [
    {
      "name": "Michal Koutny",
      "email": "michal@fykos.cz",
      "role": "FKSDB Creator"
    },
    {
      "name": "Organisers of FYKOS.cz",
      "email": "webmaster@fykos.cz"
    }
  ],
  "autoload": {
    "psr-4": {
      "FKSDB\\": "app/",
      "FKSDB\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "initTestDatabase": [
      "./tests/init-database.sh"
    ],
    "clearTestDatabase": [
      "./tests/clear-database.sh"
    ],
    "clearTestCache": [
      "rm -rf temp/tester/"
    ],
    "clearCache": [
      "rm -rf temp/cache/"
    ],
    "test": [
      "@clearTestCache",
      "@clearTestDatabase",
      "vendor/bin/tester tests/ -p php -s -c tests/php.ini -j 8"
    ],
    "testCoverage": [
      "@clearTestCache",
      "@clearTestDatabase",
      "vendor/bin/tester tests -p php --coverage coverage.html --coverage-src app -s -c tests/php.ini -j 8"
    ],
    "testPSR12": [
      "php -e vendor/squizlabs/php_codesniffer/bin/phpcs --extensions=php,phpt --standard=PSR12 --parallel=8 app/ tests/"
    ],
    "testPHPStan": [
      "vendor/bin/phpstan analyse app tests libs"
    ],
    "testPHPStanDev": [
      "vendor/bin/phpstan analyse -l7 app/Models/ORM/Models"
    ]
  }
}

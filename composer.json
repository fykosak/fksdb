{
  "description": "",
  "name": "fykosak/fksdb",
  "require": {
    "php": ">=7.4.0 <8.0.0",
    "ext-gettext": "*",
    "ext-mbstring": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-soap": "*",
    "ext-dom": "*",
    "ext-xsl": "*",
    "nette/utils": "^3.2.3",
    "nette/security": "^3.1.4",
    "nette/finder": "^2.5.2",
    "nette/http": "^3.1.2",
    "nette/robot-loader": "^3.4.1",
    "nette/safe-stream": "^2.4.2",
    "nette/component-model": "^3.0.1",
    "nette/mail": "^3.1.7",
    "nette/neon": "^3.2.2",
    "nette/forms": "^3.1.4",
    "nette/php-generator": "^3.5.3",
    "nette/database": "^3.1.3",
    "nette/caching": "^3.1.1",
    "latte/latte": "^2.10.3",
    "tracy/tracy": "^2.8.7",
    "nette/tester": "^2.4.1",
    "nette/application": "^3.1.3",
    "nette/di": "^3.0.9",
    "nette/bootstrap": "^3.1.1",
    "nette/tokenizer": "^3.1.0",
    "nette/nette": "^3.1.0",
    "setasign/fpdf": "1.8.2",
    "setasign/fpdi": "1.5.4",
    "league/oauth2-google": "3.0.3",
    "fykosak/nette-orm": "dev-master#eb50e74",
    "fykosak/nette-fks-utils": "dev-master#443965e"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.6.0"
  },
  "authors": [
    {
      "name": "Michal Koutny",
      "email": "michal@fykos.cz",
      "role": "FKSDB Creator"
    },
    {
      "name": "Organisers of FYKOS.cz",
      "email": "webmaster@fykos.cz"
    }
  ],
  "autoload": {
    "psr-4": {
      "FKSDB\\": "app/",
      "FKSDB\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "initTestDatabase": [
      "./tests/init-database.sh"
    ],
    "clearTestDatabase": [
      "./tests/clear-database.sh"
    ],
    "clearTestCache": [
      "rm -rf temp/tester/"
    ],
    "clearCache": [
      "rm -rf temp/cache/"
    ],
    "test": [
      "@clearTestCache",
      "@clearTestDatabase",
      "vendor/bin/tester tests -p php -s -c tests/php.ini -j 8"
    ],
    "testCoverage": [
      "@clearTestCache",
      "@clearTestDatabase",
      "vendor/bin/tester tests -p php --coverage coverage.html --coverage-src app -s -c tests/php.ini -j 8"
    ]
  }
}

{
  "require": {
    "php": ">=7.4.0",
    "nette/utils": "2.5.4",
    "nette/security": "2.4.4",
    "nette/finder": "2.5.2",
    "nette/http": "2.4.11",
    "nette/robot-loader": "3.1.1",
    "nette/safe-stream": "2.4.1",
    "nette/component-model": "2.4.0",
    "nette/mail": "2.4.6",
    "nette/neon": "3.0.1",
    "nette/forms": "2.4.10",
    "nette/reflection": "2.4.2",
    "nette/php-generator": "3.4.1",
    "nette/database": "2.4.10",
    "nette/caching": "2.5.9",
    "latte/latte": "2.8.1",



    "tracy/tracy": "2.6.8",



    "nette/tester": "2.3.3",
    "nette/application": "~2.4.0",
    "setasign/fpdf": "^1.8",
    "setasign/fpdi": "1.5.4",
    "nette/di": "2.4.16",
    "nette/bootstrap": "2.4.6",
    "nette/tokenizer": "2.3.0"
  },
  "autoload": {
    "classmap": []
  },
  "scripts": {
    "initTestDatabase": [
      "./tests/init-database.sh"
    ],
    "clearTestDatabase": [
      "./tests/clear-database.sh"
    ],
    "clearTestCache": [
      "rm -rf temp/tester/"
    ],
    "clearCache": [
      "rm -rf temp/cache/"
    ],
    "test": [
      "@clearTestCache",
      "@clearTestDatabase",
      "vendor/bin/tester tests -p php -s -c tests/php.ini"
    ],
    "testCoverage": [
      "@clearTestCache",
      "@clearTestDatabase",
      "vendor/bin/tester tests -p php --coverage coverage.html --coverage-src app -s -c tests/php.ini"
    ]
  }
}

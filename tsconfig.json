{
  "compilerOptions": {
    "outDir": "/dist",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "baseUrl": ".",
    "lib": [
      "esnext",
      "dom"
    ],
    "paths": {
      "FKSDB/*": [
        "./app/*"
      ],
      "vendor/*": [
        "./vendor/*"
      ],
      "@translator/*": [
        "./app/Models/Localization/*"
      ]
    }
  },
  "include": [
    "./app/**/*",
    "./vendor/fykosak/**/*"
  ]
}
